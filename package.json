{
  "name": "mangosteen-fe",
  "private": true,
  "version": "0.36.0",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite -c vite.build.config.ts build",
    "preview": "vite preview",
    "prepare": "pnpm gen-svg-map",
    "gen-exp-index": "tsx src/scripts/gen-export-index.ts",
    "gen-svg-map": "pnpm run svgo && tsx src/batch/gen-svg-map.ts",
    "svgo": "svgo -f ./src/assets/icons -o ./src/assets/icons/filter --config=./svgo.config.cjs",
    "svgo-color": "svgo -f ./src/assets/icons -o ./src/assets/icons/filter"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.10",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "@vueuse/router": "^10.7.2",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "pinia": "^2.1.7",
    "superjson": "^2.2.1",
    "svgo": "^3.2.0",
    "svgstore": "^3.0.1",
    "swiper": "^11.0.5",
    "vant": "^4.8.3",
    "vite-plugin-svgo": "^1.4.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.11.5",
    "@vant/auto-import-resolver": "^1.0.2",
    "@vitejs/plugin-vue": "^4.6.2",
    "axios-mock-adapter": "^1.22.0",
    "rollup-plugin-gzip": "^3.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.7.10",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.12",
    "vite-bundle-analyzer": "^0.8.0",
    "vite-plugin-inspect": "^0.7.42",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "7.0.11",
    "vue-tsc": "^1.8.27"
  }
}
